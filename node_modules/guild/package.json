{
    "name": "guild",
    "version": "1.2.3",
    "description": "An in-memory node.js cache with random eviction skewed to evict least-used items",
    "scripts": {
        "test": "npm run unit-tests",
        "unit-tests": "./node_modules/mocha/bin/mocha -R spec --recursive ./test/unit",
        "perf-tests": "./node_modules/mocha/bin/mocha -R spec --recursive ./test/performance",
        "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- ./test/unit -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "generate-docs": "rm -r ./jsdoc/*; echo \"Generating Documentation...\\n\"; ./node_modules/.bin/jsdoc -r -d ./jsdoc ./lib"
    },
    "devDependencies": {
        "chai": "1.9.0",
        "istanbul": "^0.3.0",
        "mocha": "1.21.5",
        "mockery": "1.4.0",
        "sinon": "1.10.3",
        "coveralls": "2.11.1",
        "jsdoc": "3.2.2"
    },
    "keywords": [
        "cache",
        "in-memory",
        "eviction",
        "random",
        "non-deterministic",
        "least frequently used",
        "LFU"
    ],
    "license": "MIT"
}
